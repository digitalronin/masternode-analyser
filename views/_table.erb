<table cellspacing="0" id='dataTable'>
  <thead>
    <tr>
      <th>Coin</th>
      <th>Price ($)</th>
      <th>Change (%)</th>
      <th>Volume ($)</th>
      <th>Market Cap. ($)</th>
      <th>RoI (%)</th>
      <th>Masternodes</th>
      <th>Coins / MN</th>
      <th>Masternode Value ($)</th>
    </tr>
  </thead>
  <tbody>
    <% coins.each do |coin| %>
      <%= erb :_coin, locals: { coin: coin } %>
    <% end %>
  </tbody>
</table>
